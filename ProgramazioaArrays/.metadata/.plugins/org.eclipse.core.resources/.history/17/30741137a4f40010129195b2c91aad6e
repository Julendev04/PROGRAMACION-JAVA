package UD6_Tanda1;

import java.util.Iterator;

import Arrays.Kontsola;

public class Ariketa1 {
	static final int N = 4;
	
	public static void main(String[] args) {
		int [] notak;
		notak=new int[N];
		for (int i = 0; i < notak.length; i++) {
			System.out.println("Sartu"+(i+1)+"ikaslearen nota:");
			notak[i]=Kontsola.irakurriInt();
		}
		
		for (int i = 0; i < notak.length; i++) {
			System.out.print(notak[i]+"\t");
			
		}
		
		
	}

}
