package Arrays;

public class Ordenatu {

    public static void main(String[] args) {

        int[] arr = new int[10];
        for (int i = 0; i < 5; i++) {
            arr[i] = 10*i;
        }

        int tam = 5;
        System.out.println("Sartu nahi duzun zenbakia:");
        int zenbakia=Kontsola.irakurriInt();
        for (int i = 0; i<5; i++) {
            System.out.print(arr[i] + "\t");
        }
    }

    public static int Posizioa(int[] arr, int n, int tam) {
        int pos = 0;
        for (int i = 0; i < tam; i++) {
            if (n > arr[i]) {
                pos = i + 1;
            }
        }
        return pos;
    }
}

