package Arrays;

public class Adibideak {
	int [] arr;
	public static void main(String[] args) {
		int[] arr={12, 10, 5, 1, 7, 22};
		
		for (int i = 0; i < arr.length; i++) {
			System.out.print(arr[i]+"\t");
		}
		
		for (int i=0; i<arr.length;i++) {
			if (arr[i]%2==0) {
				arr[i]=10;
			}else {
				arr[i]=20;
			}
			System.out.print(arr[i]+"\t");
		}
	}

}
